{
  "AppSettings": {
    "ElmahConfig": {
      "LogFolder": "AppData/ElmahLogs"
    },
    "UserCredentials": {
      "Username": "admin",
      "Password": "password",
      "Secret": "secret"
    },
    "UploadPath": "Uploads", // Same as docker volume from docker-compose.yml. Case sensetive on Linux!!!
